<%= simple_form_for(instance) do |f| %>
	<%= f.error_notification %>

	<div class="form-inputs">
	<%= f.hidden_field :parent_collection_id %>
	<%= label_tag(:name, "Name") %>
	<%= f.text_field :name %>
	<%= render 'instance_form', f: f, form_definition: form_definition, form: instance.form %>
	</div>

	<div class="form-actions">
    <%= f.button :submit %>
  	</div>
<% end %>