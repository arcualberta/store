<p id="notice"><%= notice %></p>

<h1>Metadata Sets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @metadata_sets.each do |metadata_set| %>
      <tr>
        <td><%= link_to 'Show', metadata_set %></td>
        <td><%= link_to 'Edit', edit_metadata_set_path(metadata_set) %></td>
        <td><%= link_to 'Destroy', metadata_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Metadata Set', new_metadata_set_path %>
